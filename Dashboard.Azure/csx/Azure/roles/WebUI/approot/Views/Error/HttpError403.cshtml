@model WebUI.Models.Error.ErrorDetails
@{
    Layout = "~/Views/Shared/_Dashboard.cshtml";
    ViewBag.Title = this.Response.StatusCode.ToString();
    ViewBag.Subtitle = this.Response.StatusDescription;
}
@section Styles
{
    <link href="@Url.Content("~/Content/metro/pages/error.css")" rel="stylesheet" />
}
<div class="row-fluid">
    <div class="span12">
        <div class="row-fluid page-500">
            <div class="span5 number">403</div>
            <div class="span7 details">
                <h3>There's nothing to see here...</h3>
                <p>
                    Well, actually there is but you're not allowed.<br />
                    If this is somethign you should have access to, please contact the administrator.
                </p>
            </div>
        </div>
    </div>
</div>