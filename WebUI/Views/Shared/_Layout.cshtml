<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />

    <meta content="" name="author" />
    <meta content="" name="description" />
    <meta content="" name="keywords" />

    <link href="@Url.Content("~/Content/favicon.ico")" rel="icon" />
    <link href="@Url.Content("~/Content/favicon.png")" rel="shortcut icon" />

    <!-- Global Styles -->
    @Styles.Render("~/Content/bootstrap")
    <link href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Content/metro")
    @Styles.Render("~/Content/metro/default-theme")
    <link href="@Url.Content("~/Scripts/uniform/css/uniform.default.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/gritter/css/jquery.gritter.css")" rel="stylesheet" type="text/css" />
    <!-- /Global Styles -->

    <!-- Page-Specific Styles -->
    @RenderSection("Styles", required: false)
    <!-- /Page-Specific Styles -->
</head>
<body class="@ViewBag.BodyCssClass">
    @RenderBody()

    <!-- Global Scripts -->
    @Scripts.Render("~/Scripts/jquery")
    @Scripts.Render("~/Scripts/bootstrap")<!-- must be loaded after jQueryUI -->
    <!--[if lt IE 9]>
    @Scripts.Render("~/Scripts/ltie9")
    <![endif]-->
    <script src="@Url.Content("~/Scripts/breakpoints/jquery.breakpoints.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-slimscroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.blockui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/placeholder/jquery.placeholder.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/uniform/jquery.uniform.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/App.js")" type="text/javascript"></script>
    <!-- /Global Scripts -->

    @RenderSection("Scripts", required: false)
</body>
</html>
