@model WebUI.Models.UI.KOProfile
@Scripts.Render(WebUI.BundleNames.KockoutJS_ScriptBundle)
<script type="text/javascript">
    (function(){
        @Html.Raw("var profile = " + Json.Encode(Model) + ";")
        var ProfileVM = ko.mapping.fromJS(profile);
        ko.applyBindings(ProfileVM);
    })();
</script>
